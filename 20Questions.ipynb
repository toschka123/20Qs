    "    {NegFilters}\n",
    "    }} \n",
    "    group by ?p ?o \n",
    "    order by desc(?count)\n",
    "    limit 25\n",
    "    \"\"\"\n",
    "        \n",
    "    return FilterQuestion\n",
    "\n",
    "\n"
