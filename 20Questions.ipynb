    "\n",
    "\n",
    "  \n",
    "def updateQuery( PosFilters, NegFilters):\n",
    "\n",
    "    FilterQuestion =  f\"\"\"\n",
    "    select (count(*) as ?count) ?p ?o  where \n",
    "    {{\n",
    "\n",
